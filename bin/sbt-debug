#!/bin/bash

SCADS_CONFIG=config/test.conf java -XX:MaxGCPauseMillis=1000 -XX:+UseConcMarkSweepGC -noverify -XX:ReservedCodeCacheSize=64M -noverify -Djava.library.path=/usr/local/mesos/lib/java -Xss2M -Xmx4G -XX:MaxPermSize=3G -Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5005 -jar `dirname $0`/sbt-launch-0.11.0-SNAPSHOT.jar "$@"  
