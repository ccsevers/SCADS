<queryruns>

    <query name="myNetworks" class="user" primarykey="1">
        <inputs/>
        <outputs class="network">
            <output primarykey="1"/>
            <output primarykey="5"/>
        </outputs>
    </query>

    <query name="postsOnMyWall" class="user" primarykey="1">
        <inputs/>
        <outputs class="wallpost" inorder="true">
            <output primarykey="2"/>
            <output primarykey="4"/>
            <output primarykey="10"/>
        </outputs>
    </query>

    <query name="myPostsOnOtherWalls" class="user" primarykey="3">
        <inputs/>
        <outputs class="wallpost" inorder="true">
            <output primarykey="6"/>
        </outputs>
    </query>

    <query name="myPostsOnUserWall" class="user" primarykey="4">
        <inputs>
            <input type="int">2</input>
        </inputs>
        <outputs class="wallpost" inorder="true">
            <output primarykey="12"/>
            <output primarykey="13"/>
        </outputs>
    </query>

    <query name="myFriends" class="user" primarykey="1">
        <inputs/>
        <outputs class="user">
            <output primarykey="2"/>
            <output primarykey="3"/>
            <output primarykey="5"/>
        </outputs>
    </query>

    <query name="pendingFriendRequests" class="user" primarykey="1">
        <inputs/>
        <outputs class="user">
            <output primarykey="4"/>
        </outputs>
    </query>

    <query name="myGroups" class="user" primarykey="3">
        <inputs/>
        <outputs class="user">
            <output primarykey="1"/>
            <output primarykey="2"/>
        </outputs>
    </query>

    <query name="groupWall" class="user" primarykey="1">
        <inputs/>
        <outputs class="wallpost" inorder="true">
            <output primarykey="14"/>
            <output primarykey="15"/>
        </outputs>
    </query>

    <query name="myPokedBy" class="user" primarykey="2">
        <inputs/>
        <outputs class="user">
            <output primarykey="1"/>
        </outputs>
    </query>


</queryruns>
